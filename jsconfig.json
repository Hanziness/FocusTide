{
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "strict": true,
    "types": [
      "@types/node",
      "@pinia/nuxt"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
